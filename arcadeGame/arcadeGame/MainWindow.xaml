<Window x:Class="arcadeGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:arcadeGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas Name="GameCanvas" Focusable="True">
        <Label Name="HealthShow">Health: 5</Label>
        <Button Canvas.Bottom="12" Canvas.Left="50" Height="40" Width="100" Click="InstaKill_Click">Instakill!!</Button>
        <Button Canvas.Bottom="12" Canvas.Left="650" Height="40" Click="Button_Click">hit me if you can</Button>
    </Canvas>
</Window>
